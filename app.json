{
  "expo": {
    "name": "ShopeMe",
    "slug": "ShopeMe",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./src/assets/logo.png",
      "backgroundColor": "#430064"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./scr/assets/notification.png",
          "color": "#430064"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bilalhadid.ShopeMe"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
